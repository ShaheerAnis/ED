<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cc"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F8F6E5"
    tools:context=".drawer">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="800dp"
                android:background="#F8F6E5">

                <Button
                    android:id="@+id/dbutton11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="16dp"
                    android:background="@drawable/profiletextbackk"
                    android:drawableLeft="@drawable/baseline_developer_board_24"
                    android:drawableRight="@drawable/baseline_arrow_forward_ios_24"
                    android:drawablePadding="10dp"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:paddingRight="20dp"
                    android:text="My projects"
                    android:textAllCaps="false"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/dbutton10" />

                <Button
                    android:id="@+id/dbutton10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="16dp"
                    android:background="@drawable/profiletextbackk"
                    android:drawableLeft="@drawable/baseline_view_list_24"
                    android:drawableRight="@drawable/baseline_arrow_forward_ios_24"
                    android:drawablePadding="10dp"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:paddingRight="20dp"
                    android:text="My List"
                    android:textAllCaps="false"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/dcardView6" />

                <Button
                    android:id="@+id/dbutton9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="50dp"
                    android:layout_marginTop="32dp"
                    android:layout_marginEnd="50dp"
                    android:background="@drawable/profiletextbackk"
                    android:drawableLeft="@drawable/baseline_logout_24"
                    android:drawablePadding="10dp"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:paddingRight="20dp"
                    android:text="Sign Out"
                    android:textAlignment="center"
                    android:textAllCaps="true"
                    android:textColor="#B11313"
                    android:textSize="18sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/dbutton8" />

                <Button
                    android:id="@+id/dbutton8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="16dp"
                    android:background="@drawable/profiletextbackk"
                    android:drawableLeft="@drawable/baseline_message_24"
                    android:drawableRight="@drawable/baseline_arrow_forward_ios_24"
                    android:drawablePadding="10dp"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:paddingRight="20dp"
                    android:text="Chat"
                    android:textAllCaps="false"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/dbutton11" />

                <TextView
                    android:id="@+id/dtextView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:text="My Account"
                    android:textColor="@color/black"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/dtextView51"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Profile setting and more"
                    android:textColor="#776E6E"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="@+id/dtextView3"
                    app:layout_constraintStart_toStartOf="@+id/dtextView3"
                    app:layout_constraintTop_toBottomOf="@+id/dtextView3" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/dcardView6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    app:cardCornerRadius="15dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/dtextView51">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:background="#43AF47" >

                        <TextView
                            android:id="@+id/textView84"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="8dp"
                            android:text="Phone"
                            android:textColor="@color/white"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="@+id/cardView24"
                            app:layout_constraintStart_toEndOf="@+id/cardView24"
                            app:layout_constraintTop_toTopOf="@+id/cardView24"
                            app:layout_constraintVertical_bias="0.75" />

                        <TextView
                            android:id="@+id/textView76"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:text="Name"
                            android:textColor="@color/white"
                            android:textSize="22sp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="@+id/cardView24"
                            app:layout_constraintStart_toEndOf="@+id/cardView24"
                            app:layout_constraintTop_toTopOf="@+id/cardView24"
                            app:layout_constraintVertical_bias="0.04000002" />

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView24"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_marginStart="16dp"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="30dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <ImageView
                                android:id="@+id/imageView38"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:scaleType="fitCenter"
                                app:srcCompat="@drawable/adminphoto" />
                        </androidx.cardview.widget.CardView>

                        <Button
                            android:id="@+id/button10"
                            android:layout_width="wrap_content"
                            android:layout_height="26dp"
                            android:layout_marginEnd="8dp"
                            android:layout_marginBottom="2dp"
                            android:background="#43AF47"
                            android:text="Update"
                            android:textColor="@color/white"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent" />

                        <TextView
                            android:id="@+id/textView85"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"
                            android:text="country"
                            android:textColor="@color/bacgroundd"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/imageView39"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="2dp"
                            app:layout_constraintBottom_toBottomOf="@+id/textView85"
                            app:layout_constraintEnd_toStartOf="@+id/textView85"
                            app:layout_constraintTop_toTopOf="@+id/textView85"
                            app:srcCompat="@drawable/baseline_outlined_flag_24" />

                        <TextView
                            android:id="@+id/textView87"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="-10dp"
                            android:layout_marginEnd="2dp"
                            android:text="Gender"
                            android:textColor="@color/white"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/textView76" />

                        <ImageView
                            android:id="@+id/imageView45"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            app:layout_constraintBottom_toTopOf="@+id/textView87"
                            app:layout_constraintEnd_toEndOf="@+id/textView87"
                            app:layout_constraintStart_toStartOf="@+id/textView87"
                            app:srcCompat="@drawable/genderr" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <ProgressBar
                    android:id="@+id/progressBar7"
                    style="?android:attr/progressBarStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.528"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/dbutton9" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>